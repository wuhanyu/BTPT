<!DOCTYPE html>
<html>
	<head>
		<title>缺陷修复时间预测自动化原型系统</title>
		<%= stylesheet_link_tag    "application", media: "all", "data-turbolinks-track" => true %>
		<%= csrf_meta_tags %>
	</head>
	<body>
		<header class="header white-bg">
			<div class="sidebar-toggle-box">
				<div data-original-title="Toggle Navigation" data-placement="right" class="icon-reorder tooltips"></div>
			</div>
			<!--logo start-->
			<a href="index.html" class="logo">FBT<span>System</span></a>
			<!--logo end-->
			<div class="top-nav ">
				<!--search & user info start-->
				<ul class="nav pull-right top-menu">
					<li>
						<input type="text" class="form-control search" placeholder="Search">
					</li>
					<!-- user login dropdown start-->
					<li class="dropdown">
						<a data-toggle="dropdown" class="dropdown-toggle" href="#"> <img alt="" src="assets/avatar1_small.jpg"> <span class="username">欢迎, 吴含宇</span> <b class="caret"></b> </a>
						<ul class="dropdown-menu extended logout">
							<div class="log-arrow-up"></div>
							<li>
								<a href="#"><i class=" icon-suitcase"></i>Profile</a>
							</li>
							<li>
								<a href="#"><i class="icon-cog"></i> Settings</a>
							</li>
							<li>
								<a href="#"><i class="icon-bell-alt"></i> Notification</a>
							</li>
							<li>
								<a href="login.html"><i class="icon-key"></i> Log Out</a>
							</li>
						</ul>
					</li>
					<!-- user login dropdown end -->
				</ul>
				<!--search & user info end-->
			</div>
		</header>
		<%= render "side"%>
		<section id="container" >
			<%= yield %>
		</section>
		<!--main content end-->
		<!--footer start-->
		<footer class="site-footer">
			<div class="text-center">
				2014 &copy; Simon Wu
				<a href="#" class="go-top"> <i class="icon-angle-up"></i> </a>
			</div>
		</footer>
		<!--footer end-->
	</body>
	<%= javascript_include_tag "application", "data-turbolinks-track" => true %>
	<script>
		//owl carousel

		$(document).ready(function() {
			$("#owl-demo").owlCarousel({
				navigation : true,
				slideSpeed : 300,
				paginationSpeed : 400,
				singleItem : true,
				autoPlay : true

			});
		});

		//custom select box

		$(function() {
			$('select.styled').customSelect();
		});

	</script>
</html>
